   let num2 = Number(prompt("Digite o 2º número"))
   let num3 = Number(prompt("Digite o 3º número"))
   let quadrado = (num1 **2) 
   let quadrado2 = (num2 **2)
   let quadrado3 = (num3 **2) 
   console.log(`O numero ${num1} ao quadrado é ${quadrado}\n O número ${num2} ao quadrado é ${quadrado2}\n O número ${num3} ao quadrado é ${quadrado3}`)
   

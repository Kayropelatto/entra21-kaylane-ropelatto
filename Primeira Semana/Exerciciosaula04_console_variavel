18) 
    let raio = Number(prompt("Insira o valor do raio"))
    let PI = 3.14159
    let formula = (4 / 3) * PI * (raio **3)
    console.log(`O resultado é ${formula.toFixed(3)}`)
    
19)
    let base = Number(prompt("Insira o valor da base do triângulo"))
    let altura = Number(prompt("Insira o valor da altura do triângulo"))
    let area = (base * altura) / 2
    console.log(`A área desse triângulo é de ${area.toFixed(2)}`)

20)
    let preco = Number(prompt("Insira o vaor do produto"))
    let percentual = Number(prompt("Insira o percentual de desconto"))
    let valordesconto = (percentual / 100) * preco
    let resultado = preco - valordesconto
    console.log(`Total ${resultado} \nDesconto ${valordesconto}`)

21)
    let nome = prompt("Insira o nome do vendedor")
    let salariofixo = Number(prompt("Insira o salário fixo"))
    let totalvendas = Number(prompt("Insira o total de vendas"))
    let comissao = (totalvendas / 100) * 15
    let resultado = salariofixo + comissao
    console.log(`O s{nome} recebeu R$ ${resultado.toFixed(2)}`)
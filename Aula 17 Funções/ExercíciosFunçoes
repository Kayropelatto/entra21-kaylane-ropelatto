3)
        function potencia(a, b) {
            return a ** b
        }
        let a = +prompt("Digite um número")
        let b = +prompt("Digite o exponencial")

        let resultado = potencia(a, b)
        console.log(`O número ${a} elevado a ${b} é igual a ${resultado}`)





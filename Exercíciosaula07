21) 
    // DANDO ERRO
        let turno = +prompt("Digite o turno que você estuda:\nM-Matutino\nV-Vespertino\nN-NOTURNO").toUpperCase();
        switch(turno) {
            case "M":
                console.log("Bom dia!")
            break
            case "V": 
                console.log("Boa tarde!")
            break
            case "N": 
                console.log("Boa noite")
            break
            default:
                console.log("Valor Inválido")
        }

        CORREÇÃO: 

        let horario = prompt("Em que turno você estuda?\nConsidere:\nM - matutino\nV - vespertino\nN - noturno").toUpperCase();
    switch (horario) {
    case "M":
        alert("Bom dia!");
        break;
    case "V":
        alert("Boa tarde!")
        break;
    case "N":
        alert("Boa noite");
        break;
    default:
        alert("O valor digitado é inválido");
}

13) 
        let numero = +prompt("Digite um número")
        if (numero > 0) {
            let raiz = Math.sqrt(numero)
            console.log(`O resultado da raiz é ${raiz.toFixed(2)}`)
        } else {
            let quadrado = numero ** 2
            console.log(`O resulado do quadrado desse número é ${quadrado.toFixed(2)}`)
        }


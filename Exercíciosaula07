10) 
        let lado1 = +prompt("Valor de um lado do triângulo")
        let lado2 = +prompt("Valor de outro lado do triângulo")
        let lado3 = +prompt("Valor do último lado do triângulo")
        if (lado1 != lado2 && lado3 != lado1 && lado2 != lado3 ) {
            console.log("É um triângulo Escaleno")
        } else if (lado1 === lado2 && lado1 != lado3 && lado2 != lado3|| lado1 === lado3 && lado1 != lado2 || lado2 === lado3 && lado2 != lado1 && lado3 != lado1) {
            console.log("É um triângulo Isósceles")
        } else {
            console.log("É um triângulo Equilátero")
        }

11) 
        let numero = +prompt("Digite um número")
        if (numero > 0) {
            let raiz = Math.sqrt(numero)
            console.log(`A raiz de ${numero} é ${raiz.toFixed(2)}`)
        } else {
            console.log("Número Inválido")
        }


7) 
Boolean(A > 10 && B <= 20) // false
Boolean(A > 10 || B <= 20) // true
Boolean((A === 10 || B > 20) && B === 20) // true
console.log(null || 2 || undefined) // 2
Boolean(0 && 1) // false

8)
        let valorA = Number(prompt("Digite o 1º valor"))
        let valorB = Number(prompt("Digite o 2º valor"))
        let valorC = Number(prompt("Digite o 3º valor"))
        if (valorA > valorB && valorA > valorC) {
            console.log(`O maior número é ${valorA}`)
        } else if (valorB > valorA && valorB > valorC) {
            console.log(`O maior número é o ${valorB}`)
        } else {
            console.log(`O maior número é o ${valorC}`)
        }

